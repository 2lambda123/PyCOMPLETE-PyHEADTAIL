mkdir ../python3_futurize2
rm -rf ../python3_futurize2/PyHEADTAIL
cp -r ../python3_futurize1/PyHEADTAIL ../python3_futurize2/PyHEADTAIL
rm ../python3_futurize2/PyHEADTAIL/*.so

cd ../python3_futurize2/PyHEADTAIL

futurize --stage2 -w *.py **/*.py **/**/*.py **/**/**/*.py **/**/**/**/*.py **/**/**/**/**/*.py > ../../PyHEADTAIL/futurize_stage2.out


rm -rf .git

echo""
echo "***The code generated by this script is not tested!"
echo "***Use it at your own risk!"
echo''