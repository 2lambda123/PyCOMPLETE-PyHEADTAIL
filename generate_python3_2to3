mkdir ../python3_2to3
rm -rf ../python3_2to3/PyHEADTAIL
cp -r ../PyHEADTAIL ../python3_2to3/PyHEADTAIL
rm ../python3_2to3/PyHEADTAIL/*.so

2to3 --output-dir=../python3_2to3/PyHEADTAIL -W -n ../PyHEADTAIL > 2to3.out

rm -rf ../python3_2to3/PyHEADTAIL/.git

echo""
echo "***The code generated by this script is not tested!"
echo "***Use it at your own risk!"
echo""